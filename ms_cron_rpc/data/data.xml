<odoo>
    <record id="ir_cron_rpc_transfer" model="ir.cron">
        <field name="name">Transfert RPC automatique</field>
        <field name="model_id" ref="model_rpc_settings"/>
        <field name="state">code</field>
        <field name="code">
for rec in env['rpc.settings'].search([]):
    rec.action_run_rpc_transfer()
        </field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="active">False</field>
    </record>
</odoo>
